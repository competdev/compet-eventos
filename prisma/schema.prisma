datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model obr {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model User {
  id              String  @id @default(auto()) @map("_id") @db.ObjectId
  name            String
  email           String  @unique
  data_nascimento DateTime
  telefone        String
  matricula       String
  presenca        Boolean 
}